	"item": [
		{
			"name": "Get all 200",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Check if the response is 200\r",
							"pm.test(\"Status code is 200\", () => {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"// Check if the response time is less than 200ms\r",
							"pm.test(\"Response time is less than 200ms\", () => { \r",
							"    pm.expect(pm.response.responseTime).to.be.below(200);\r",
							"});\r",
							"\r",
							"\r",
							"const questions = pm.response.json();\r",
							"\r",
							"// Check if the response is an array of questions\r",
							"pm.test(\"Response is an array\", () => {\r",
							"    pm.expect(questions).to.be.an('array');\r",
							"});\r",
							"\r",
							"// Check if each question has the required fields\r",
							"pm.test(\"Each question has required fields\", () => {\r",
							"    questions.forEach(question => {\r",
							"        pm.expect(question).to.have.property('id');\r",
							"        pm.expect(question).to.have.property('question');\r",
							"        pm.expect(question).to.have.property('options');\r",
							"        pm.expect(question).to.have.property('correctAnswer');\r",
							"        pm.expect(question).to.have.property('difficulty');\r",
							"    });\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "x-role",
						"value": "user",
						"type": "text"
					}
				],
				"url": {
					"raw": "http://localhost:3000/api/questions",
					"protocol": "http",
					"host": [
						"localhost"
					],
					"port": "3000",
					"path": [
						"api",
						"questions"
					]
				}
			},
			"response": []
		},
		{
			"name": "Get by id 200",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.collectionVariables.set(\"questionId\", 1);"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Parse response\r",
							"const question = pm.response.json();\r",
							"\r",
							"// 1. Status code is 200\r",
							"pm.test(\"Status code is 200\", () => {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"// 2. Response time < 200ms\r",
							"pm.test(\"Response time is less than 200ms\", () => {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(200);\r",
							"});\r",
							"\r",
							"// 3. Response is an object\r",
							"pm.test(\"Response is an object\", () => {\r",
							"    pm.expect(question).to.be.an('object');\r",
							"});\r",
							"\r",
							"// 4. Question has required fields\r",
							"pm.test(\"Question has required fields\", () => {\r",
							"    pm.expect(question).to.have.property('id');\r",
							"    pm.expect(question).to.have.property('question');\r",
							"    pm.expect(question).to.have.property('options');\r",
							"    pm.expect(question).to.have.property('correctAnswer');\r",
							"    pm.expect(question).to.have.property('difficulty');\r",
							"});\r",
							"\r",
							"// 5. ID matches requested ID\r",
							"pm.test(\"Returned ID matches requested ID\", () => {\r",
							"    pm.expect(question.id).to.eql(\r",
							"        parseInt(pm.collectionVariables.get(\"questionId\"))\r",
							"    );\r",
							"});\r",
							"\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "x-role",
						"value": "admin",
						"type": "text"
					}
				],
				"url": {
					"raw": "http://localhost:3000/api/questions/{{questionId}}",
					"protocol": "http",
					"host": [
						"localhost"
					],
					"port": "3000",
					"path": [
						"api",
						"questions",
						"{{questionId}}"
					]
				}
			},
			"response": []
		},
		{
			"name": "Post admin 201 created",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const question = pm.response.json();",
							"",
							"// 1. Status code 201",
							"pm.test(\"Status code is 201\", () => {",
							"    pm.response.to.have.status(201);",
							"});",
							"",
							"// 2. Response is an object",
							"pm.test(\"Response is an object\", () => {",
							"    pm.expect(question).to.be.an('object');",
							"});",
							"",
							"// 3. Question has required fields",
							"pm.test(\"Question should have required fields\", () => {",
							"    pm.expect(question).to.have.property('id');",
							"    pm.expect(question).to.have.property('question');",
							"    pm.expect(question).to.have.property('options');",
							"    pm.expect(question).to.have.property('correctAnswer');",
							"    pm.expect(question).to.have.property('difficulty');",
							"});",
							"",
							"// 4. Difficulty is medium",
							"pm.test(\"Difficulty is medium\", () => {",
							"    pm.expect(question.difficulty).to.eql(\"medium\");",
							"});",
							"",
							"// 5. Save ID for next requests",
							"pm.collectionVariables.set(\"questionId\", question.id);",
							"",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
